/def start = \
     /set wimpyminhp 150%; \
     /set wimpyminpu 100%; \
     /set wimpypercent 0%; \
     /set target a%; \
     /set wimpycommand /atoinf%; \
     /set outofhpcommand /eval $[wimpycommand]%; \
     /set outofpucommand /eval $[wimpycommand]%; \
     /set incombat no%; \
     /set ranaway 0%; \
     am

/def -p0 -mregexp -t'legs run away with you' lookafterrun = \
     /eval /set ranaway $[ranaway + 1]%; \
     /eval /repeat -$[rand(1000,2500)/1000] 1 l
/def inftoalpha = w%;s%;s%;d%;d%;w%;slide chaos%;enter%;e%;e%;d%;
/def alphatoinf = \
     u%;w%;w%;vortex%;/chaostoguild%;e%;u%;u%;n%;n%;e%;/starthealing
/def -p0 -mregexp -t'all of your wounds' donehealing = \
     /stophealing%; \
     /eval /infto$[target]%; \
     /if (incombat =~ "no") \
     	 /eval kill $[target]%; \
	 /set incombat yes%; \
     /endif

/def -p10 -F -mregexp -t'^Syd dealt the killing blow to ([a-zA-Z0-9 ]+)' getall = \
     gac%;pab%; \
     /eval /sys echo "You killed %P1 and ran away $[ranaway] times" >> /tmp/mudfeedbot%; \
     /set ranaway 0

